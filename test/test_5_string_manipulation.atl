include "std.atl"

main fn() -> i64 {
    :: test_a string = "test"
    :: test_b string = "test"
    puts("test_a: ")
    puts(test_a)
    putchar('\n')
    puts("size: "
    puti(test_a.len)
    putchar('\n')

    puts("test_a: ")
    puts(test_b)
    putchar('\n')
    puts("size: "
    puti(test_a.len)
    putchar('\n')

    :: test_c string = ""
    test_c = join(test_a, test_b)
    putchar('\n')
    puts(test_c)
    puti(test_c.len)
    putchar('\n')

    if equal(test_a, test_b) {
        puts("test_a and test_b are equal")
    } else {
        puts("test_a and test_b are not equal")
    }

    -> 0
}
